{
  "name": "xmlbind-ts",
  "version": "0.1.0",
  "description": "JAXB-like XML binding for TypeScript (scaffold)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "xsd2ts": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/xsd/cli.ts",
    "test": "jest --runInBand",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "npm run build"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "fast-xml-parser": "^4.0.0",
    "@xmldom/xmldom": "^0.9.5"
  },
  "devDependencies": {
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "ts-jest": "^29.0.0",
    "jest-junit": "^13.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/parser": "^8.46.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2"
  },
  "jest-junit": {
    "outputDirectory": "test-results",
    "outputName": "jest-junit.xml"
  }
}
